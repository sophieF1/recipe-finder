{
  "name": "recipe-finder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve-fe": "vue-cli-service serve",
    "serve-be": "node be/index.js",
    "build": "vue-cli-service build",
    "local": "npm run serve-be && npm run serve-fe",
    "db": "node db/dbService.js",
    "test:unit": "vue-cli-service test:unit",
    "test-be": "mocha --require babel-register be/tests/*.js --exit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bluebird": "^3.7.1",
    "chai-fetch-mock": "^2.0.0",
    "core-js": "^3.3.2",
    "cors": "^2.8.5",
    "fetch-mock": "^7.7.3",
    "mocha": "^6.2.2",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "sqlite3": "^4.1.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-unit-mocha": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "mocha": "^6.2.2",
    "null-loader": "^3.0.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^1.1.1",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
